@model object

<td>
    <a asp-action="Details" class="btn btn-info" asp-route-id="@Model">
        <i class="fa-solid fa-circle-info"></i>
        Details
    </a>
</td>
<td>
    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model">
        <i class="fa-solid fa-pen-to-square"></i>
        Edit
    </a>
</td>
<td>
    <a asp-action="Delete" class="btn btn-danger" asp-route-id="@Model">
        <i class="fa-solid fa-trash"></i>
        Delete
    </a>
</td>
